// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Projects {
  id          String @id @default(uuid())
  name        String
  email       String @unique
  phone       Int
  service     String
  projectName String
  Information String
}

model JoinUs {
  id      String @id @default(uuid())
  name    String
  email   String @unique
  phone   Int
  major   String
  message String
}
